@model IEnumerable<FinalDraft.Model.Main>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InspectionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdditionalId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DoorCard)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Describe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrimaryPhoneIs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LocationCon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Equipment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InspectionComments)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.States)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Postal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CrctAdd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AddrsExplaination)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GeoCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PropertyPhoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mapcode)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InspectionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdditionalId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoorCard)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Describe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrimaryPhoneIs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocationCon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Equipment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InspectionComments)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Country)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.States)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Postal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CrctAdd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddrsExplaination)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeoCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PropertyPhoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mapcode)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
